SELECT * FROM clothing_store_inventory;
SELECT * FROM clothing_store_inventory ORDER BY price;
SELECT color, SUM(price) FROM clothing_store_inventory GROUP BY color;
SELECT aisle, MIN(price) FROM clothing_store_inventory GROUP BY aisle;
